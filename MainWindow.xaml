<Window x:Class="Space_Invaders_game.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Space_Invaders_game"
        mc:Ignorable="d"
        Title="Space Invaders" Height="500" Width="800">
    <Window.Background>
        <ImageBrush ImageSource="imagens/galaxy_bg.jpg" />
    </Window.Background>

    <Canvas Name="myCanvas" Focusable="True" KeyDown="KeyIsDown" KeyUp="KeyIsUp">
        <Label Name="enemiesLeft" Foreground="white" FontSize="16" FontWeight="ExtraBold">Enimigos que faltam: 0</Label>

        <Rectangle Name="player" Fill="White" Height="65" Width="55" Canvas.Left="381" Canvas.Top="394"></Rectangle>
    </Canvas>
</Window>
